SELECT ?source ?sourceLabel ?type ?typeLabel 
                WHERE {
                ?source wdt:P31/wdt:P279* wd:Q482994 ;
                   wdt:P175 ?type.
                  ?type ?label "Nirvana"@en .
                 SERVICE wikibase:label {
                      bd:serviceParam wikibase:language "en".
                 }
                 
                }
           LIMIT 10
